		function loadNotifications() {
			var PendingLDK = $("#PendingLDK");

			$.ajax({
				method: 'POST',
				headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
				url: "@myurl" + 'LDK/INQn',
