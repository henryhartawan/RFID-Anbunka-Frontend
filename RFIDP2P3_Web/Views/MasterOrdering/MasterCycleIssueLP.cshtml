@{
	ViewData["Title"] = "Master Cycle Issue LP";
	string myurl = new ConfigurationBuilder().AddJsonFile("appsettings.json").Build().GetSection("Path:URL").Value;
}

<style>
	.k-dropdown-wrap .k-input, .k-numeric-wrap .k-input, .k-picker-wrap .k-input {
		height: 30px;
	}
</style>

<div class="page-main">
	<div class="page-content padding-5">
		<div id="grid" style="margin-top:5px; margin-bottom:5px;"></div>
	</div>
</div>

<div class="modal fade modal-primary" id="editor" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
	<div class="" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title" id="myModalTitle">Master Cycle Issue LP Trucking</h4>
			</div>
			<div class="modal-body margin-bottom-0 padding-bottom-0">
				<div class="panel">
					<div class="panel-body">
						<form class="form-horizontal">
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Route</label>
								<div class="col-sm-9">
									<input type="text" class="" id="Route" style="width:100%" required />
								</div>
							</div>
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Dock</label>
								<div class="col-sm-9">
									<input type="text" class="" id="Dock" style="width:100%" required />
								</div>
							</div>
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Operation</label>
								<div class="col-sm-9">
									<input type="text" class="" id="Operation" style="width:100%;" required />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Cycle Arrival</label>
								<div class="col-sm-9">
									<input type="text" class="" id="CycleArrival" style="width:100%" required />
								</div>
							</div>
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Time Arrival</label>
								<div class="col-sm-9">
									<input type="text" class="" id="TimeArrival" style="width:100%" required />
								</div>
							</div>
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Implement Date</label>
								<div class="col-sm-9">
									<input type="text" class="" id="ImplementDate" style="width:100%" required />
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal" id='clear'>Close</button>
				<button type="button" class="btn btn-primary" id="btnSave">Submit</button>
			</div>
		</div>
	</div>
</div>

<div class="modal fade modal-primary" id="editorUpload" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="padding-top:25vh;">
	<div class="" role="document" style="padding-left:25vw;">
		<div class="modal-content" style="width:50vw">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title" id="myModalTitle">Upload Master Cycle Issue LP Trucking</h4>
			</div>
			<div class="modal-body margin-bottom-0 padding-bottom-0">
				<form id="formUpload" enctype="multipart/form-data">
					<span>Download template <a href="../../templates/Master Cycle Issue LP Trucking.xlsx" style="color:#0099ff" download="Template Master Cycle Issue LP Trucking.xlsx">here</a></span>
					<input type="file" name="file" class="form-control" id="fileUpload" accept=".xls,.xlsx" />
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal" id='clear'>Close</button>
				<button type="button" class="btn btn-primary" id="btnSaveUpload" value="Upload">Upload</button>
			</div>
		</div>
	</div>
</div>

<script>
	var IUType = 'I';
	var CycleIssueId = '';

	var AppD = {
		attachEvents: function () {
			var o = this;
			o.dom.$btnSave.on('click', o.eventHandlers.onClickBtnSave.bind(o));
			o.dom.$btnSaveUpload.on('click', o.eventHandlers.onClickBtnSaveUpload.bind(o));

			o.dom.$grid.on('click', '#btnCreate', o.eventHandlers.onClickBtnCreate.bind(o));
			o.dom.$grid.on('click', '#btnUpload', o.eventHandlers.onClickBtnUpload.bind(o));
			o.dom.$grid.on('click', '#btnExcel', o.eventHandlers.onClickBtnExcel.bind(o));

			o.dom.$grid.on('click', '.btnUpdate', o.eventHandlers.onClickBtnUpdate.bind(o));
			o.dom.$grid.on('click', '.btnDelete', o.eventHandlers.onClickBtnDelete.bind(o));
			o.dom.$grid.on('click', '.btnActive', o.eventHandlers.onClickBtnActive.bind(o));
			o.dom.$grid.on('click', '.btnDeactive', o.eventHandlers.onClickBtnActive.bind(o));
		},
		cacheDom: function () {
			var o = this;
			o.dom.$editor = $("#editor");
			o.dom.$grid = $("#grid");
			o.dom.$editorUpload = $("#editorUpload");
			o.dom.$btnSaveUpload = $("#btnSaveUpload");
			o.dom.$btnSave = $("#btnSave");
		},
		data: {},
		dom: {},
		eventHandlers: {
			onClickBtnCreate: function () {
				var o = this;
				IUType = 'I';
				CycleIssueId = '';

				$("#Route").data('kendoComboBox').value('');
				$("#Route").data("kendoComboBox").enable(true);
				$("#Dock").data('kendoComboBox').value('');
				$("#Operation").data('kendoDropDownList').value('');
				$("#CycleArrival").data('kendoNumericTextBox').value('');
				$("#TimeArrival").data('kendoTimePicker').value('');
				$("#ImplementDate").data('kendoDatePicker').value('');

				o.dom.$editor.modal('show');
			},
			onClickBtnDelete: function (e) {
				var o = this;
				var di = o.dom.$grid.data('kendoGrid').dataItem($(e.target).closest('tr'));

				swal.fire({
					title: 'Delete?',
					text: "You won't be able to revert this!",
					icon: 'warning',
					showCancelButton: true,
					confirmButtonText: 'Yes'
				}).then((result) => {
					if (result.isConfirmed) {
						$.ajax({
							method: 'post',
							headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
							url: "@myurl"+'MasterCycleIssueLP/DEL',
							dataType: 'text',
							data: JSON.stringify({
								CycleIssueID: di.CycleIssueID,
								DockCode: di.DockCode
							})
						}).done(function (data, textStatus, jqXHR) {
							if (data !== 'success') {
								swal.fire('', data, 'error');
								return;
							}
							swal.fire({ title: 'Success', text: 'Data deleted', icon: 'success', timer: 2000, showConfirmButton: false });
						}).fail(function (jqXHR, textStatus, errorThrown) {
							swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
						}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
							o.dom.$grid.data('kendoGrid').dataSource.read();
						});
					}
				});
			},
			onClickBtnActive: function (e) {
				var o = this;
				var di = o.dom.$grid.data('kendoGrid').dataItem($(e.target).closest('tr'));

				swal.fire({
					title: di.CycleIssueStatus == 'Active' ? 'Deactivate?' : 'Activate?',
					icon: 'warning',
					showCancelButton: true,
					confirmButtonText: 'Yes'
				}).then((result) => {
					if (result.isConfirmed) {
						$.ajax({
							method: 'post',
							headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
							url: "@myurl"+'MasterCycleIssueLP/ACT',
							dataType: 'text',
							data: JSON.stringify({
								CycleIssueID: di.CycleIssueID,
								DockCode: di.DockCode,
								UserLogin: "@Context.Session.GetString("PIC_ID")"
							})
						}).done(function (data, textStatus, jqXHR) {
							if (data !== 'success') {
								swal.fire('', data, 'error');
								return;
							}
							swal.fire({ title: 'Success', text: di.CycleIssueStatus == 'Active' ? 'Data Inactive' : 'Data Active', icon: 'success', timer: 2000, showConfirmButton: false });
						}).fail(function (jqXHR, textStatus, errorThrown) {
							swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
						}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
							o.dom.$grid.data('kendoGrid').dataSource.read();
						});
					}
				});
			},
			onClickBtnExcel: function () {
				var o = this;
				o.dom.$grid.data('kendoGrid').saveAsExcel();
			},
			onClickBtnUpdate: function (e) {
				var o = this;
				IUType = 'U';
				var di = o.dom.$grid.data('kendoGrid').dataItem($(e.target).closest('tr'));
				CycleIssueId = di.CycleIssueID;

				$("#Route").data('kendoComboBox').value(di.RouteCode);
				$("#Route").data("kendoComboBox").enable(false);
				$("#Dock").data('kendoComboBox').value(di.DockCode);
				$("#Dock").data("kendoComboBox").enable(false);
				$("#Operation").data('kendoDropDownList').value(di.Operation);
				$("#CycleArrival").data('kendoNumericTextBox').value(di.CycleArrival);
				$("#TimeArrival").data('kendoTimePicker').value(di.TimeArrival);
				$("#ImplementDate").data('kendoDatePicker').value(di.ImplementDate);

				o.dom.$editor.modal('show');

				setTimeout(function() {
					$("#Dock").data("kendoComboBox").enable(false);
				}, 500);
			},
			onClickBtnSave: function (e) {
				var o = this;

				var Dock = $("#Dock").data('kendoComboBox').value();
				var Route = $("#Route").data('kendoComboBox').value();
				var Operation = $("#Operation").data('kendoDropDownList').value();
				var CycleArrival = $("#CycleArrival").val();
				var TimeArrival = kendo.toString($("#TimeArrival").data('kendoTimePicker').value(),'HH:mm:ss');
				var ImplementDate = kendo.toString($("#ImplementDate").data('kendoDatePicker').value(),'yyyy-MM-dd');

				if(Dock != '' && Route != '' && Operation != '' && CycleArrival != '' && TimeArrival != '' && TimeArrival != null && ImplementDate != '' && ImplementDate != null){
					swal.fire({
						title: "Save?",
						icon: 'warning',
						showCancelButton: true,
						confirmButtonText: 'Yes'
					}).then((result) => {
						if (result.isConfirmed) {
							$.ajax({
								method: 'post',
								headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
								url: "@myurl" + 'MasterCycleIssueLP/INS',
								dataType: 'text',
								data: JSON.stringify({
									CycleIssueID: CycleIssueId,
									DockCode: Dock,
									RouteCode: Route,
									Operation: Operation,
									CycleArrival: CycleArrival,
									TimeArrival: TimeArrival,
									ImplementDate: ImplementDate,
									IUType: IUType,
									UserLogin: "@Context.Session.GetString("PIC_ID")"
								})
							}).done(function (data, textStatus, jqXHR) {
								if (data !== 'success') {
									swal.fire('Error', data, 'error');
									return;
								}
								o.dom.$editor.modal('hide');
								swal.fire({ title: 'Success', text: 'Data saved', icon: 'success', timer: 2000, showConfirmButton: false });
							}).fail(function (jqXHR, textStatus, errorThrown) {
								swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
							}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
								o.dom.$grid.data('kendoGrid').dataSource.read();
							});
						}
					});
				}else{
					swal.fire({ title: 'Warning', text: 'Please fill all fields', icon: 'warning'});
				}
			},
			onClickBtnUpload: function () {
				var o = this;
				$("#fileUpload").data('kendoUpload').clearAllFiles();
				o.dom.$editorUpload.modal('show');
			},
			onClickBtnSaveUpload: function () {
				var o = this;
				swal.fire({
					title: 'Upload Master Cycle Issue LP Trucking?',
					icon: 'warning',
					showCancelButton: true,
					confirmButtonText: 'Yes'
				}).then((result) => {
					if (result.isConfirmed) {
						let formData = new FormData();
						let files = $("#fileUpload").data('kendoUpload').getFiles();
						let file = files[0].rawFile;
						formData.append('file', file);

						$.ajax({
							url: "@myurl" + "MasterCycleIssueLP/Upload?UID=" + "@Context.Session.GetString("PIC_ID")",
							headers: { 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
							type: 'post',
							data: formData,
							contentType: false,
							processData: false
						}).done(function (data, textStatus, jqXHR) {
							var remarks = data[0].Remarks;
							if(remarks != '') {
								swal.fire({ title: 'Warning', text: remarks, icon: 'warning'});
							} else {
								o.dom.$editorUpload.modal('hide');
								swal.fire({ title: 'Success', text: 'Data Uploaded', icon: 'success', timer: 2000, showConfirmButton: false });
								$("#fileUpload").data('kendoUpload').clearAllFiles();
							}
						}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
							$("#grid").data('kendoGrid').dataSource.read();
							$("#fileUpload").data('kendoUpload').clearAllFiles();
						});
					}
				});
			},
		},
		init: function () {
			var o = this;
			o.cacheDom();
			o.attachEvents();
			o.initElements();
		},
		initElements: function () {
			var o = this;

			o.dom.$grid.kendoGrid({
				excel: {
					fileName: "Master Cycle Issue LP Trucking.xlsx",
					allPages: true,
					filterable: true
				},
				dataSource: {
					transport: {
						read: function (options) {
							$.ajax({
								method: 'post',
								headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
								url: "@myurl"+'MasterCycleIssueLP/INQ',
								dataType: 'text',
								data: {}
							}).done(function (data, textStatus, jqXHR) {
								try {
									var result = JSON.parse(data);
								}
								catch (e) {
									swal.fire('Error', 'Error loading grid', 'error');
									options.success({ data: [] });
									return;
								}
								options.success(result);
							}).fail(function (jqXHR, textStatus, errorThrown) {
								swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
								options.success({ data: [] });
							});
						}
					},
					schema: {
						model: {
							id: "CycleIssueID",
							fields: {
								CycleIssueID: { type: "string" },
								Dock: { type: "string" },
								RouteCode: { type: "string" },
								Operation: { type: "string" },
								CycleArrival: { type: "string" },
								TimeArrival: { type: "string" },
								ImplementDate: { type: "string" },
								CycleIssueStatus: { type: "string" },
								LastUpdate: { type: "string" },
								UserUpdate: { type: "string" },
								Remarks: { type: "string" },
							}
						}
					},
					pageSize: 20
				},
				scrollable: true,
				sortable: true,
				resizable: true,
				reorderable: true,
				filterable: {
					extra: false,
					operators: {
						string: {
							contains: "Contains",
							eq: "Equal to",
							neq: "Not equal to"
						}
					}
				},
				pageable: {
					refresh: true,
					input: true,
					numeric: true,
					pageSizes: true,
					buttonCount: 5
				},
				columns: [
				@if (Context.Session.GetString("edit_master_cycle_issue_lp") == "True")
				{
					<text>
					{
						width: 50,
						command: [{
							name: "Update",
							text: "<i class='fa fa-pencil-square-o btnUpdate' style='font-size:18px;color:#737373;'></i>"
						}]
					},
					</text>
				}
				@if (Context.Session.GetString("del_master_cycle_issue_lp") == "True")
				{
					<text>
					{
						width: 50,
						command: [{
							name: "Delete",
							text: "<i class='fa fa-trash-o btnDelete' style='font-size:18px;color:#800000;'></i>"
						}]
					},
					</text>
				}
				@if (Context.Session.GetString("edit_master_cycle_issue_lp") == "True")
				{
					<text>
					{
						width: 50,
						command: [{
							name: "Active",
							text: "<i class='fa fa-check btnActive' style='font-size:18px;color:#008000;'></i>"
						},{
							name: "Deactive",
							text: "<i class='fa fa-times btnDeactive' style='font-size:18px;color:#ff0000;'></i>"
						}]
					},
					</text>
				}
				{
					field: "CycleIssueID",
					title: "ID",
					width: 150
				},
				{
					field: "RouteCode",
					title: "Route",
					width: 150
				},
				{
					field: "Dock",
					title: "Dock",
					width: 150
				},
				{
					field: "Operation",
					title: "Operation",
					width: 150
				},
				{
					field: "CycleArrival",
					title: "Cycle Arrival",
					width: 150
				},
				{
					field: "TimeArrival",
					title: "Time Arrival",
					width: 150
				},
				{
					field: "ImplementDate",
					title: "Implement Date",
					width: 150
				},
				{
					field: "CycleIssueStatus",
					title: "Status",
					width: 100
				},
				{
					title: "Last Update",
					columns: [
					{
						field: "UserUpdate",
						title: "User",
						width: 100
					},
					{
						field: "LastUpdate",
						title: "Date",
						width: 150
					}]
				}],
				toolbar: $("#template_toolbar").html(),
				dataBound: function(e){
					var grid = this;

					grid.tbody.find("tr[role='row']").each(function(){
						var model = grid.dataItem(this);
						if(model.CycleIssueStatus == 'Active'){
							$(this).find(".k-grid-Active").hide();
							$(this).find(".k-grid-Deactive").show();
						}else{
							$(this).find(".k-grid-Active").show();
							$(this).find(".k-grid-Deactive").hide();
						}
					});
				}
			});

			$("#Route").kendoComboBox({
				dataValueField: "Route",
				dataTextField: "ViewValue",
				filter: "contains",
				dataSource: {
					transport: {
						read: function (options) {
							$.ajax({
								method: 'post',
								headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
								url: "@myurl" + 'DropDown/INQ',
								dataType: 'text',
								data: JSON.stringify({ Ftype: "Route", Plant: "" })
							}).done(function (data, textStatus, jqXHR) {
								try {
									var result = JSON.parse(data);
								}
								catch (e) {
									swal.fire('Error', 'Error loading grid', 'error');
									options.success({ data: [] });
									return;
								}
								options.success(result);
							}).fail(function (jqXHR, textStatus, errorThrown) {
								swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
								options.success({ data: [] });
							});
						}
					}
				},
				change: function(){
					if (this.value() && this.selectedIndex == -1) {
						this.text("");
						this.value(null);
					}

					$("#TimeArrival").data('kendoTimePicker').value('');
				}
			}).data("kendoComboBox");

			$("#Dock").kendoComboBox({
				cascadeFrom: "Route",
				autoBind: false,
				dataValueField: "IdValue",
				dataTextField: "ViewValue",
				filter: "contains",
				enable: false,
				dataSource: {
					transport: {
						read: function (options) {
							$.ajax({
								method: 'post',
								headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
								url: "@myurl" + 'DropDown/INQ',
								dataType: 'text',
								data: JSON.stringify({ Ftype: "DockRoute", Plant: "" })
							}).done(function (data, textStatus, jqXHR) {
								try {
									var result = JSON.parse(data);
								}
								catch (e) {
									swal.fire('Error', 'Error loading grid', 'error');
									options.success({ data: [] });
									return;
								}
								options.success(result);
							}).fail(function (jqXHR, textStatus, errorThrown) {
								swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
								options.success({ data: [] });
							});
						}
					}
				},
				change: function(){
					if (this.value() && this.selectedIndex == -1) {
						this.text("");
						this.value(null);
					}

					$("#TimeArrival").data('kendoTimePicker').value('');
				}
			}).data("kendoComboBox");

			$("#CycleArrival").kendoNumericTextBox({
				format: "#",
				min: 0,
				decimals: 0,
				restrictDecimals: true,
				change: function(e){
					$("#TimeArrival").data('kendoTimePicker').value('');
				},
				spin: function(e){
					$("#TimeArrival").data('kendoTimePicker').value('');
				}
			}).data("kendoNumericTextBox");

			$("#TimeArrival").kendoTimePicker({
				format: "HH:mm",
				change: function(e){
					var operation = $("#Operation").data('kendoDropDownList').value();
					var time = this.value();
					var min = (function(){var a = new Date(); a.setHours(7,0,0,0); return a;})();
					var max = (function(){var a = new Date(); a.setHours(19,59,59,999); return a;})();

					var status = 0;
					if(operation == 'Day'){
						if(time > max || time < min) {
							swal.fire('Warning', 'Arrival time outside of operation time', 'warning');
							$("#TimeArrival").data('kendoTimePicker').value('');
							status = 1;
						}
					} else if(operation == 'Night') {
						if(time >= min && time <= max) {
							swal.fire('Warning', 'Arrival time outside of operation time', 'warning');
							$("#TimeArrival").data('kendoTimePicker').value('');
							status = 1;
						}
					}

					if(status != 1) {
						var Dock = $("#Dock").data('kendoComboBox').value();
						var Route = $("#Route").data('kendoComboBox').value();
						var Operation = $("#Operation").data('kendoDropDownList').value();
						var CycleArrival = $("#CycleArrival").val();
						var TimeArrival = kendo.toString($("#TimeArrival").data('kendoTimePicker').value(),'HH:mm:ss');

						if(Dock != '' && Route != '' && Operation != '' && CycleArrival != '') {
							$.ajax({
								method: 'post',
								headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
								url: "@myurl" + 'MasterCycleIssueLP/CheckTime',
								dataType: 'text',
								data: JSON.stringify({
									DockCode: Dock,
									RouteCode: Route,
									Operation: Operation,
									CycleArrival: CycleArrival,
									TimeArrival: TimeArrival
								})
							}).done(function (data, textStatus, jqXHR) {
								if(data !== 'success') {
									swal.fire('Warning', data, 'warning');
									$("#TimeArrival").data('kendoTimePicker').value('');
								}
							}).fail(function (jqXHR, textStatus, errorThrown) {
								swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
								console.log(jqXHR);
								$("#TimeArrival").data('kendoTimePicker').value('');
							});
						}
					}
				}
			}).data("kendoTimePicker");

			$("#fileUpload").kendoUpload({
				multiple: false,
				validation: {
					allowedExtensions: [".xls", "xlsx"]
				}
			});

			$("#Operation").kendoDropDownList({
				dataSource: [
					{ id: "Day", name: "Day"},
					{ id: "Night", name: "Night"},
				],
				dataValueField: "id",
				dataTextField: "name",
				change: function(e){
					$("#TimeArrival").data('kendoTimePicker').value('');
				}
			}).data("kendoDropDownList");

			$("#ImplementDate").kendoDatePicker({
				format: "dd/MM/yyyy"
			});
		}
	};

	$(function () {
		AppD.init();
	});
</script>

