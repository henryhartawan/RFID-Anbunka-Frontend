@{
	ViewData["Title"] = "Master ID Part";
	string myurl = new ConfigurationBuilder().AddJsonFile("appsettings.json").Build().GetSection("Path:URL").Value;
}

<style>
	.k-dropdown-wrap .k-input, .k-numeric-wrap .k-input, .k-picker-wrap .k-input {
		height: 30px;
	}
</style>

<div class="page-main">
	<div class="page-content padding-5">
		<div id="grid" style="margin-top:5px; margin-bottom:5px;"></div>
	</div>
</div>

<div class="modal fade modal-primary" id="editor" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
	<div class="" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title" id="myModalTitle">Master ID Part</h4>
			</div>
			<div class="modal-body margin-bottom-0 padding-bottom-0">
				<div class="panel">
					<div class="panel-body">
						<form class="form-horizontal">
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Dock</label>
								<div class="col-sm-9">
									<input type="text" class="" id="Dock" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Depth</label>
								<div class="col-sm-9">
									<input type="text" class="" id="Depth" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6">
								<label class="col-sm-3 control-label">Part</label>
								<div class="col-sm-9">
									<input type="text" class="" id="Part" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Capacity Min (Box)</label>
								<div class="col-sm-9">
									<input type="text" class="kendoNumber" id="MinCapacity" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Capacity Max (Box)</label>
								<div class="col-sm-9">
									<input type="text" class="kendoNumber" id="MaxCapacity" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Order Time</label>
								<div class="col-sm-9">
									<input type="text" class="form-control" id="OrderTime" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Cycle Issue X</label>
								<div class="col-sm-9">
									<input type="text" class="kendoNumber" id="CIX" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Cycle Issue Y</label>
								<div class="col-sm-9">
									<input type="text" class="kendoNumber" id="CIY" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Cycle Issue Z</label>
								<div class="col-sm-9">
									<input type="text" class="kendoNumber" id="CIZ" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Effective Date</label>
								<div class="col-sm-9">
									<input type="text" class="" id="EffectiveDate" style="width:100%;" />
								</div>
							</div>
							<div class="form-group form-material col-md-6" style="height:37px;">
								<label class="col-sm-3 control-label">Effective Shift</label>
								<div class="col-sm-9">
									<input type="text" class="" id="EffectiveShift" style="width:100%;" />
								</div>
							</div>
							<div class="clearfix"></div>
						</form>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal" id='clear'>Close</button>
				<button type="button" class="btn btn-primary" id="btnSave">Submit</button>
			</div>
		</div>
	</div>
</div>

<div class="modal fade modal-primary" id="editorUpload" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="padding-top:25vh;">
	<div class="" role="document" style="padding-left:25vw;">
		<div class="modal-content" style="width:50vw">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title" id="myModalTitle">Upload Master ID Part</h4>
			</div>
			<div class="modal-body margin-bottom-0 padding-bottom-0">
				<form id="formUpload" enctype="multipart/form-data">
					<span>Download template <a href="../../templates/Master ID Part.xlsx" style="color:#0099ff" download="Template Master ID Part.xlsx">here</a></span>
					<input type="file" name="file" class="form-control" id="fileUpload" accept=".xls,.xlsx" />
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal" id='clear'>Close</button>
				<button type="button" class="btn btn-primary" id="btnSaveUpload" value="Upload">Upload</button>
			</div>
		</div>
	</div>
</div>

<script>
	var IUType = 'I';
	var exCore = '';

	var AppD = {
		attachEvents: function () {
			var o = this;
			o.dom.$btnSave.on('click', o.eventHandlers.onClickBtnSave.bind(o));
			o.dom.$btnSaveUpload.on('click', o.eventHandlers.onClickBtnSaveUpload.bind(o));

			o.dom.$grid.on('click', '#btnCreate', o.eventHandlers.onClickBtnCreate.bind(o));
			o.dom.$grid.on('click', '#btnUpload', o.eventHandlers.onClickBtnUpload.bind(o));
			o.dom.$grid.on('click', '#btnExcel', o.eventHandlers.onClickBtnExcel.bind(o));

			o.dom.$grid.on('click', '.btnUpdate', o.eventHandlers.onClickBtnUpdate.bind(o));
			o.dom.$grid.on('click', '.btnDelete', o.eventHandlers.onClickBtnDelete.bind(o));
			o.dom.$grid.on('click', '.btnActive', o.eventHandlers.onClickBtnActive.bind(o));
			o.dom.$grid.on('click', '.btnDeactive', o.eventHandlers.onClickBtnActive.bind(o));
		},
		cacheDom: function () {
			var o = this;
			o.dom.$editor = $("#editor");
			o.dom.$grid = $("#grid");
			o.dom.$editorUpload = $("#editorUpload");
			o.dom.$btnSaveUpload = $("#btnSaveUpload");
			o.dom.$btnSave = $("#btnSave");
		},
		data: {},
		dom: {},
		eventHandlers: {
			onClickBtnCreate: function () {
				var o = this;
				IUType = 'I';
				exCore = "";

				$("#Depth").data('kendoComboBox').value('');
				$("#Part").data('kendoComboBox').value('');
				$("#Dock").data('kendoComboBox').value('');
				$("#Dock").data('kendoComboBox').enable(true);
				$("#MinCapacity").data('kendoNumericTextBox').value('');
				$("#MaxCapacity").data('kendoNumericTextBox').value('');
				$("#OrderTime").val('');
				$("#CIX").data('kendoNumericTextBox').value('');
				$("#CIY").data('kendoNumericTextBox').value('');
				$("#CIZ").data('kendoNumericTextBox').value('');
				$("#EffectiveDate").data('kendoDateTimePicker').value('');
				$("#EffectiveShift").data('kendoDropDownList').value('');

				o.dom.$editor.modal('show');
			},
			onClickBtnDelete: function (e) {
				var o = this;
				var di = o.dom.$grid.data('kendoGrid').dataItem($(e.target).closest('tr'));

				swal.fire({
					title: 'Delete?',
					text: "You won't be able to revert this!",
					icon: 'warning',
					showCancelButton: true,
					confirmButtonText: 'Yes'
				}).then((result) => {
					if (result.isConfirmed) {
						$.ajax({
							method: 'post',
							headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
							url: "@myurl"+'MasterIDPart/DEL',
							dataType: 'text',
							data: JSON.stringify({
								ExCore: di.ExCore
							})
						}).done(function (data, textStatus, jqXHR) {
							if (data !== 'success') {
								swal.fire('', data, 'error');
								return;
							}
							swal.fire({ title: 'Success', text: 'Data deleted', icon: 'success', timer: 2000, showConfirmButton: false });
						}).fail(function (jqXHR, textStatus, errorThrown) {
							swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
						}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
							o.dom.$grid.data('kendoGrid').dataSource.read();
						});
					}
				});
			},
			onClickBtnActive: function (e) {
				var o = this;
				var di = o.dom.$grid.data('kendoGrid').dataItem($(e.target).closest('tr'));

				swal.fire({
					title: di.IDPartStatus == 'Active' ? 'Deactivate?' : 'Activate?',
					icon: 'warning',
					showCancelButton: true,
					confirmButtonText: 'Yes'
				}).then((result) => {
					if (result.isConfirmed) {
						$.ajax({
							method: 'post',
							headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
							url: "@myurl"+'MasterIDPart/ACT',
							dataType: 'text',
							data: JSON.stringify({
								ExCore: di.ExCore,
								UserLogin: "@Context.Session.GetString("PIC_ID")"
							})
						}).done(function (data, textStatus, jqXHR) {
							if (data !== 'success') {
								swal.fire('', data, 'error');
								return;
							}
							swal.fire({ title: 'Success', text: di.IDPartStatus == 'Active' ? 'Data Inactive' : 'Data Active', icon: 'success', timer: 2000, showConfirmButton: false });
						}).fail(function (jqXHR, textStatus, errorThrown) {
							swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
						}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
							o.dom.$grid.data('kendoGrid').dataSource.read();
						});
					}
				});
			},
			onClickBtnExcel: function () {
				var o = this;
				o.dom.$grid.data('kendoGrid').saveAsExcel();
			},
			onClickBtnUpdate: function (e) {
				var o = this;
				IUType = 'U';
				var di = o.dom.$grid.data('kendoGrid').dataItem($(e.target).closest('tr'));
				exCore = di.ExCore;

				$("#Depth").data('kendoComboBox').value(di.DepthID);
				$("#Depth").data('kendoComboBox').enable(false);
				$("#Part").data('kendoComboBox').value(di.PartOrderID);
				$("#Part").data('kendoComboBox').enable(false);
				$("#Dock").data('kendoComboBox').value(di.DockCode);
				$("#Dock").data('kendoComboBox').enable(false);
				$("#MinCapacity").data('kendoNumericTextBox').value(di.MinCapacity);
				$("#MaxCapacity").data('kendoNumericTextBox').value(di.MaxCapacity);
				$("#OrderTime").val(di.OrderTime);
				$("#CIX").data('kendoNumericTextBox').value(di.CycleIssueX);
				$("#CIY").data('kendoNumericTextBox').value(di.CycleIssueY);
				$("#CIZ").data('kendoNumericTextBox').value(di.CycleIssueZ);
				$("#EffectiveDate").data('kendoDateTimePicker').value(di.EffectiveDate);
				$("#EffectiveShift").data('kendoDropDownList').value(di.EffectiveShift);

				o.dom.$editor.modal('show');

				setTimeout(function() {
					$("#Part").data('kendoComboBox').enable(false);
					$("#Depth").data('kendoComboBox').enable(false);
				}, 500);
			},
			onClickBtnSave: function (e) {
				var o = this;
				var Depth = $("#Depth").data('kendoComboBox').value();
				var Part = $("#Part").data('kendoComboBox').value();
				var Dock = $("#Dock").data('kendoComboBox').value();
				var MinCapacity = $("#MinCapacity").val();
				var MaxCapacity = $("#MaxCapacity").val();
				var OrderTime = $("#OrderTime").val();
				var CIX = $("#CIX").val();
				var CIY = $("#CIY").val();
				var CIZ = $("#CIZ").val();
				var EffectiveDate = kendo.toString($("#EffectiveDate").data('kendoDateTimePicker').value(), "yyyy-MM-dd HH:mm:ss");
				var EffectiveShift = $("#EffectiveShift").data('kendoDropDownList').value();

				if(Depth != '' && Part != '' && Dock != '' && MinCapacity != '' && MinCapacity != 0 &&
				  MaxCapacity != '' && MaxCapacity != 0 && OrderTime != '' && OrderTime != null && CIX != '' && CIX != 0 &&
				  CIY != '' && CIY != 0 && CIZ != '' && CIZ != 0 && EffectiveDate != '' && EffectiveDate != null && EffectiveShift != ''){
					swal.fire({
						title: "Save?",
						icon: 'warning',
						showCancelButton: true,
						confirmButtonText: 'Yes'
					}).then((result) => {
						if (result.isConfirmed) {
							$.ajax({
								method: 'post',
								headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
								url: "@myurl" + 'MasterIDPart/INS',
								dataType: 'text',
								data: JSON.stringify({
									ExCore: exCore,
									DepthID: Depth,
									PartOrderID: Part,
									DockCode: Dock,
									MinCapacity: MinCapacity,
									MaxCapacity: MaxCapacity,
									OrderTime: OrderTime,
									CycleIssueX: CIX,
									CycleIssueY: CIY,
									CycleIssueZ: CIZ,
									EffectiveDate: EffectiveDate,
									EffectiveShift: EffectiveShift,
									IUType: IUType,
									UserLogin: "@Context.Session.GetString("PIC_ID")"
								})
							}).done(function (data, textStatus, jqXHR) {
								if (data !== 'success') {
									swal.fire('Error', data, 'error');
									return;
								}
								o.dom.$editor.modal('hide');
								swal.fire({ title: 'Success', text: 'Data saved', icon: 'success', timer: 2000, showConfirmButton: false });
							}).fail(function (jqXHR, textStatus, errorThrown) {
								swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
							}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
								o.dom.$grid.data('kendoGrid').dataSource.read();
							});
						}
					});
				}else{
					swal.fire({ title: 'Warning', text: 'Please fill all fields', icon: 'warning'});
				}
			},
			onClickBtnUpload: function () {
				var o = this;
				$("#fileUpload").data('kendoUpload').clearAllFiles();
				o.dom.$editorUpload.modal('show');
			},
			onClickBtnSaveUpload: function () {
				var o = this;
				swal.fire({
					title: 'Upload Master ID Part?',
					icon: 'warning',
					showCancelButton: true,
					confirmButtonText: 'Yes'
				}).then((result) => {
					if (result.isConfirmed) {
						let formData = new FormData();
						let files = $("#fileUpload").data('kendoUpload').getFiles();
						let file = files[0].rawFile;
						formData.append('file', file);

						$.ajax({
							url: "@myurl" + "MasterIDPart/Upload?UID=" + "@Context.Session.GetString("PIC_ID")",
							headers: { 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
							type: 'post',
							data: formData,
							contentType: false,
							processData: false
						}).done(function (data, textStatus, jqXHR) {
							var remarks = data[0].Remarks;
							if(remarks != '') {
								swal.fire({ title: 'Warning', text: remarks, icon: 'warning'});
							} else {
								o.dom.$editorUpload.modal('hide');
								swal.fire({ title: 'Success', text: 'Data Uploaded', icon: 'success', timer: 2000, showConfirmButton: false });
								$("#fileUpload").data('kendoUpload').clearAllFiles();
							}
						}).always(function (data_or_jqXHR, textStatus, jqXHR_or_errorThrown) {
							$("#grid").data('kendoGrid').dataSource.read();
							$("#fileUpload").data('kendoUpload').clearAllFiles();
						});
					}
				});
			},
		},
		init: function () {
			var o = this;
			o.cacheDom();
			o.attachEvents();
			o.initElements();
		},
		initElements: function () {
			var o = this;

			o.dom.$grid.kendoGrid({
				excel: {
					fileName: "Master ID Part.xlsx",
					allPages: true,
					filterable: true
				},
				dataSource: {
					transport: {
						read: function (options) {
							$.ajax({
								method: 'post',
								headers: { 'Content-Type': 'application/json', 'XApiKey': 'pgH7QzFHJx4w46fI~5Uzi4RvtTwlEXp' },
								url: "@myurl"+'MasterIDPart/INQ',
								dataType: 'text',
								data: {}
							}).done(function (data, textStatus, jqXHR) {
								try {
									var result = JSON.parse(data);
								}
								catch (e) {
									swal.fire('Error', 'Error loading grid', 'error');
									options.success({ data: [] });
									return;
								}
								options.success(result);
							}).fail(function (jqXHR, textStatus, errorThrown) {
								swal.fire('Error', (jqXHR.responseText !== '' ? ': ' + jqXHR.responseText : ''), 'error');
								options.success({ data: [] });
							});
						}
					},
					schema: {
						model: {
							id: "ExCore",
							fields: {
								ExCore: { type: "string" },
								Depth: { type: "string" },
								Part: { type: "string" },
								Dock: { type: "string" },
								MinCapacity: { type: "string" },
								MaxCapacity: { type: "string" },
								OrderTime: { type: "string" },
								CycleIssueX: { type: "string" },
								CycleIssueY: { type: "string" },
								CycleIssueZ: { type: "string" },
								EffectiveDateView: { type: "string" },
								EffectiveShift: { type: "string" },
								IDPartStatus: { type: "string" },
								LastUpdate: { type: "string" },
								UserUpdate: { type: "string" },
								Remarks: { type: "string" },
							}
						}
					},
					pageSize: 20
				},
				scrollable: true,
				sortable: true,
				resizable: true,
				reorderable: true,
				filterable: {
					extra: false,
					operators: {
						string: {
							contains: "Contains",
							eq: "Equal to",
							neq: "Not equal to"
						}
					}
				},
				pageable: {
					refresh: true,
					input: true,
					numeric: true,
					pageSizes: true,
					buttonCount: 5
				},
				columns: [
				@if (Context.Session.GetString("edit_master_id_part") == "True")
				{
					<text>
					{
						width: 50,
						command: [{
							name: "Update",
							text: "<i class='fa fa-pencil-square-o btnUpdate' style='font-size:18px;color:#737373;'></i>"
						}]
					},
					</text>
				}
				@if (Context.Session.GetString("del_master_id_part") == "True")
				{
					<text>
					{
						width: 50,
						command: [{
							name: "Delete",
							text: "<i class='fa fa-trash-o btnDelete' style='font-size:18px;color:#800000;'></i>"
						}]
					},
					</text>
				}
				@if (Context.Session.GetString("edit_master_id_part") == "True")
				{
					<text>
					{
						width: 50,
						command: [{
							name: "Active",
							text: "<i class='fa fa-check btnActive' style='font-size:18px;color:#008000;'></i>"
						},{
							name: "Deactive",
							text: "<i class='fa fa-times btnDeactive' style='font-size:18px;color:#ff0000;'></i>"
						}]
					},
					</text>
				}
				{
					field: "ExCore",
					title: "Ex Core",
					width: 100
				},
				{
					field: "Depth",
					title: "Depth",
					width: 200,
					template: "<div class='customClass'> #= (Depth == null) ? ' ' : kendo.toString(Depth, '') # </div>",
				},
				{
					field: "Part",
					title: "Part",
					width: 200,
					template: "<div class='customClass'> #= (Part == null) ? ' ' : kendo.toString(Part, '') # </div>",
				},
				{
					field: "Dock",
					title: "Dock",
					width: 150,
					template: "<div class='customClass'> #= (Dock == null) ? ' ' : kendo.toString(Dock, '') # </div>",
				},
				{
					field: "MinCapacity",
					title: "Min Capacity",
					width: 100,
				},
				{
					field: "MaxCapacity",
					title: "Max Capacity",
					width: 100,
				},
				{
					field: "OrderTime",
					title: "Order Time",
					width: 100,
				},
				{
					field: "CycleIssueX",
					title: "Cycle Issue X",
					width: 100,
				},
				{
					field: "CycleIssueY",
					title: "Cycle Issue Y",
					width: 100,
				},
				{
					field: "CycleIssueZ",
					title: "Cycle Issue Z",
					width: 100,
				},
				{
					field: "EffectiveDateView",
					title: "Effective Date",
					width: 150,
					template: "<div class='customClass'> #= (EffectiveDateView == null) ? ' ' : kendo.toString(EffectiveDateView, '') # </div>",
				},
				{
					field: "EffectiveShift",
					title: "Effective Shift",
					width: 100,
				},
				{
					field: "IDPartStatus",
					title: "Status",
					width: 100
				},
				{
					title: "Last Update",
					columns: [
					{
						field: "UserUpdate",
						title: "User",
						width: 100
					},
					{
						field: "LastUpdate",
						title: "Date",
						width: 150
					}]
				}],
				toolbar: $("#template_toolbar").html(),
				dataBound: function(e){
					var grid = this;

					grid.tbody.find("tr[role='row']").each(function(){
						var model = grid.dataItem(this);
						if(model.IDPartStatus == 'Active'){
							$(this).find(".k-grid-Active").hide();
							$(this).find(".k-grid-Deactive").show();
						}else{
